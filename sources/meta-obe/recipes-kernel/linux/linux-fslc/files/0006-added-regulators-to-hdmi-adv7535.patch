From 5a3544210319b4800532231dd7b3cd7a2961ef5a Mon Sep 17 00:00:00 2001
From: Chris Storah <chris.storah@gmail.com>
Date: Fri, 9 Feb 2024 01:56:28 +0000
Subject: [PATCH] added regulators to hdmi adv7535

---
 arch/arm64/boot/dts/freescale/poc1.dtsi | 16 ++++++++++++++++
 1 file changed, 16 insertions(+)

diff --git a/arch/arm64/boot/dts/freescale/poc1.dtsi b/arch/arm64/boot/dts/freescale/poc1.dtsi
index 2c952c145a20..8fb6579737b4 100644
--- a/arch/arm64/boot/dts/freescale/poc1.dtsi
+++ b/arch/arm64/boot/dts/freescale/poc1.dtsi
@@ -185,12 +185,28 @@ hdmi@3d {
 		reg-names = "main", "cec", "edid", "packet";
 		adi,dsi-lanes = <4>;
 
+		avdd-supply = <&v1v8>;
+		dvdd-supply = <&v1v8>;
+		pvdd-supply = <&v1v8>;
+		a2vdd-supply = <&v1v8>;
+		v3p3-supply = <&v3v3>;
+		v1p2-supply = <&v1v8>;  /* 7535 requires 1V8 on this pin */
+
 		adi,input-depth = <8>;
 		adi,input-colorspace = "rgb";
 		adi,input-clock = "1x";
 		adi,input-style = <1>;
 		adi,input-justification = "evenly";
 
+		v1v8: v1v8-regulator {
+			regulator-min-microvolt = <1800000>;
+			regulator-max-microvolt = <1800000>;
+		};
+		v3v3: v3v3-regulator {
+			regulator-min-microvolt = <3300000>;
+			regulator-max-microvolt = <3300000>;
+		};
+
 		ports {
 			#address-cells = <1>;
 			#size-cells = <0>;
-- 
